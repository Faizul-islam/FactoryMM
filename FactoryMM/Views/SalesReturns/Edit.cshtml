
@model ReturnSelse

@{
    ViewData["Title"] = "Edit";
}

<form asp-controller="SalesReturns" asp-action="Edit" method="post" class="mt-3">

    <input hidden asp-for="SaleReturnId" />

    <div class="form-group row">
        <label asp-for="ProdInvId" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="ProdInvId" class="form-control" placeholder="ProdInvId" />
            <span asp-validation-for="ProdInvId" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="CustId" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="CustId" class="form-control" placeholder="CustId" />
            <span asp-validation-for="CustId" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="Quantity" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="Quantity" class="form-control" placeholder="Quantity" />
            <span asp-validation-for="Quantity" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="ErturnDate" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="ErturnDate" class="form-control" placeholder="ErturnDate" />
            <span asp-validation-for="ErturnDate" class="text-danger"></span>
        </div>
    </div>
 

    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary">Create</button>
            <a asp-controller="SalesReturns" asp-action="Index" class="btn btn-primary">Cancel</a>
        </div>
    </div>


</form>

