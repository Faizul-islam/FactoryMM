
@model Production
@{
    ViewData["Title"] = "Create";
}

<form asp-controller="Productions" asp-action="Create" method="post" class="mt-3">
    <div class="form-group row">
        <label asp-for="ProdInvId" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="ProdInvId" class="form-control" placeholder="ProdInvId" />
            <span asp-validation-for="ProdInvId" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="MatInvId" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="MatInvId" class="form-control" placeholder="MatInvId" />
            <span asp-validation-for="MatInvId" class="text-danger"></span>
        </div>
    </div>
   


    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary">Create</button>
            <a asp-controller="Productions" asp-action="Index" class="btn btn-primary">Cancel</a>
        </div>
    </div>



